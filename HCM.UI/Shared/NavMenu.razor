            {
                <MudNavGroup Title="Administration" Expanded="false" Icon="@Icons.Sharp.AdminPanelSettings" IconColor="Color.Inherit">
                    @foreach (var item in AuthMenus.Where(x => x.PMenuID == 1))
                    {
                        @if (item.CMenuName == "Approval Decesion")
                        {
                            <MudNavLink Href=@item.MenuLink Match="NavLinkMatch.Prefix" class="child-icons" Icon="@Icons.Filled.Circle">@item.CMenuName  <span class="bg-info p-2"><b>@oDocApprovalDecesionList.Count</b></span></MudNavLink>
                        }
                        else
                        {

                            <MudNavLink Href=@item.MenuLink Match="NavLinkMatch.Prefix" class="child-icons" Icon="@Icons.Filled.Circle">@item.CMenuName</MudNavLink>
